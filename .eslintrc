{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "linebreak-style": "off",
    "require-jsdoc": "off",
    "operator-linebreak": "off",
		"indent": "off",
		"no-tabs": ["error", { "allowIndentationTabs": true }]
  },
  "extends": ["eslint:recommended", "google"]
}
